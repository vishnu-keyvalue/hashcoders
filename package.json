{
  "name": "react-nextjs",
  "version": "0.0.0",
  "private": true,
  "repository": "twbs/examples",
  "license": "MIT",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "build": "next build",
    "start": "next dev",
    "lint": "next lint",
    "fix": "next lint --fix",
