    "pre-commit": "tsc --noEmit && lint-staged",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@types/node": "^20.4.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "bootstrap": "^5.3.0",
    "lint-staged": "^14.0.1",
    "next": "^13.4.12",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "tsc": "^2.0.4",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "sass": "^1.64.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ]
  }
